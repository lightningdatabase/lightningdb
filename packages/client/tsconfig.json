{
  "compilerOptions": {
    "sourceMap": false,
    "outDir": "dist",
    "strict": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["vitest/globals"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ES2020",
    "declaration": true,
    "declarationMap": false
  },
  "include": ["./src"]
}
